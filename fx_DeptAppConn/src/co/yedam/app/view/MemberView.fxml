<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>


<AnchorPane prefHeight="90.0" prefWidth="292.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.171">
   <children>
      <GridPane layoutY="13.0" prefHeight="77.0" prefWidth="394.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="95.0" minWidth="10.0" prefWidth="55.0" />
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="300.0" minWidth="10.0" prefWidth="300.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label text="Label" />
            <Label text="Label" GridPane.rowIndex="1" />
            <Label text="Label" GridPane.rowIndex="2" />
            <HBox prefHeight="25.0" prefWidth="255.0" GridPane.columnIndex="1">
            	<fx:define><ToggleGroup fx:id="group"/> </fx:define>
               <children>
                  <RadioButton mnemonicParsing="false" toggleGroup="$group" text="pieChart"  userData="pie" />
                  <RadioButton mnemonicParsing="false" toggleGroup="$group" text="barChart" userData="bar"/>
                  <RadioButton mnemonicParsing="false" toggleGroup="$group" prefHeight="16.0" prefWidth="103.0" text="lineChart" userData="line" />
               </children>
            </HBox>
         </children>
      </GridPane>
   </children>
</AnchorPane>
